{
  "name": "yetiforce/yetiforcepdf",
  "description": "Library that generate pdf files from html.",
  "type": "library",
  "keywords": [
    "pdf",
    "html",
    "html to pdf"
  ],
  "homepage": "https://github.com/YetiForceCompany/YetiForcePDF",
  "authors": [
    {
      "name": "YetiForceCompany",
      "email": "info@yetiforce.com",
      "homepage": "https://yetiforce.com/"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-mbstring": "*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-bcmath": "*",
    "ext-zlib": "*",
    "ext-gd": "*",
    "ext-filter": "*",
    "phenx/php-font-lib": "^0.5.1",
    "guzzlehttp/guzzle": "^6.3",
    "composer/ca-bundle": "^1.1",
    "milon/barcode": "^7.0.0",
    "sabberworm/php-css-parser": "^8.3"
  },
  "autoload": {
    "psr-4": {
      "YetiForcePDF\\": "lib/"
    }
  }
}
